hoppingList.jsx",[],"/Users/danielbourdier/Desktop/Code/Coding Challenges/RaysChallenge/shopping-list/src/components/mainComponents/InputContainer.jsx",["48"],"import React, { useState } from 'react'\n\n\nconst InputContainer = props => {\n  const [ name, updateName ] = useState(\"\")\n  const [ cost, updateCost ] = useState(\"\")\n\n  const { addItem: createItem } = props \n\n  // input validations to be added soon with React PropTypes\n  // This grabs entered data and adds the new item to our Parent's collection.\n  const handleSubmit = () => {\n    const newItem = {\n      'title': name,\n      'price': cost\n    }\n    createItem( currentList => [...currentList, newItem ] )\n\n  }\n\n\n  const handleUpdate = e => {\n\n    e.preventDefault()\n    let targetValue = e.target.value\n\n    switch (e.target.name) {\n      case 'item-name':\n        updateName( targetValue )\n        break;\n\n      case 'item-price':\n        updateCost( targetValue )\n        break;\n    }\n\n  }\n\n\n  return (\n    <section>\n\n      Input Container\n      <div>\n        <input \n          type=\"text\" \n          name=\"item-name\" \n          onChange={ handleUpdate } \n          placeholder=\"Add Item Name Here...\" \n        /> \n        <input \n          type=\"number\" \n          step=\"0.01\" \n          min=\"0.01\" \n          name=\"item-price\"  \n          onChange={ handleUpdate } \n          placeholder=\"Add Item Price Here...\"\n        /> \n      </div>\n\n      <div>\n        <button \n          onClick={ handleSubmit }>\n            To add a new entry\n        </button>\n      </div>\n      \n    </section>\n  )\n\n}\n\n\nexport default InputContainer",["49","50"],"/Users/danielbourdier/Desktop/Code/Coding Challenges/RaysChallenge/shopping-list/src/components/mainComponents/TotalsContainer.jsx",[],"/Users/danielbourdier/Desktop/Code/Coding Challenges/RaysChallenge/shopping-list/src/components/mainComponents/lists/CrossedListIndex.jsx",[],"/Users/danielbourdier